package core_java;

public class CoreJavaBrushUp3 {
	
	public static void main(String[] args) {
		
		//String is an object // String Literal
		// one object is created since the value is the same
		// String s = "Rahul Shetty Academy";
		String s1 = "Rahul Shetty Academy";
		
	
		
		// Two object is created since this instantiate a new object
		String s2 = new String("Welcome");
		String s3 = new String("Welcome");
		
		String s = "Rahul Shetty Academy";
		String[] splittedString = s.split("Shetty");
		
		System.out.println(splittedString[0]); // Rahul
		System.out.println(splittedString[1]); // Shetty -> Academy
		// System.out.println(splittedString[2]); // Academy
		
		System.out.println(splittedString[1].trim()); // Trim the left and right spaces
		
		for (int i = 0; i < s.length(); i++) {
			System.out.print(s.charAt(i));
		}
		
	}

}
